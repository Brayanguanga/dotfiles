

# freshshell.com
#
#
# See http://freshshell.com/readme for documentation.


# Disable default fresh binary check
FRESH_NO_BIN_CHECK=true

# Disable local files check
FRESH_NO_LOCAL_CHECK=true

# Fresh binary
fresh freshshell/fresh bin/fresh --bin=~/.fresh/.bin/fresh

# Antigen (zsh plugin manager)
fresh zsh-users/antigen antigen.zsh --file=~/.antigen/antigen.zsh

# Personal dotfiles
fresh jasonmorganson/dotfiles config/\* --file

# VIM Configuration
fresh jasonmorganson/vimrc .vimrc --file=~/.vimrc
fresh jasonmorganson/vimrc .vimrc.bundles --file=~/.vimrc.bundles

