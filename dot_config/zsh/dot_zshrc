# zmodload zsh/zprof

# Make sure that antibody is installed
(( $+commands[antibody] )) || curl -sL git.io/antibody | sh -s

# Automatic antibody static loading
if [[ ! -e "~/.zsh-plugins" ]] || [[ "~/.zsh-plugins" -nt "$XDG_CACHE_HOME/zsh/plugins.sh" ]]; then
  antibody bundle < ~/.zsh-plugins > $XDG_CACHE_HOME/zsh/plugins.sh
fi

# Load zsh plugins
source $XDG_CACHE_HOME/zsh/plugins.sh

# Hook direnv into your shell.
#eval "$(asdf exec direnv hook zsh)"

# zprof
