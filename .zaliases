alias a='fasd -a'        # any
alias s='fasd -si'       # show / search / select
alias d='fasd -d'        # directory
alias f='fasd -f'        # file

alias vi=nvim
alias git=hub

alias dotfiles='git --git-dir=$HOME/.dotfiles/.git --work-tree=$HOME'

alias update="brew update && brew upgrade; brew reinstall --HEAD neovim; brew reinstall --HEAD neovim-dot-app; antibody update; npm update -g; vim +PlugInstall! +qall"
